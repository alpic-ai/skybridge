{
  "name": "@skybridge/monorepo",
  "version": "0.0.0",
  "description": "Skybridge is a framework for building ChatGPT apps",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm --filter './packages/*' build",
    "test": "pnpm --filter './packages/*' test",
    "docs:dev": "pnpm --filter @skybridge/docs start",
    "docs:build": "pnpm --filter @skybridge/docs build",
    "docs:serve": "pnpm --filter @skybridge/docs serve",
    "version": "./scripts/version.sh",
    "publish:packages": "./scripts/publish-packages.sh",
    "publish:core": "./scripts/publish-core.sh",
    "publish:create-skybridge": "./scripts/publish-create-skybridge.sh"
  },
  "keywords": [
    "chatgpt",
    "app",
    "ai",
    "skybridge"
  ],
  "author": "Frédéric Barthelet",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@total-typescript/tsconfig": "^1.0.4",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.17.1"
}

{
  "name": "skybridge",
  "version": "0.0.0",
  "description": "Skybridge is a framework for building ChatGPT apps",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./server": {
      "types": "./dist/src/server/index.d.ts",
      "default": "./dist/src/server/index.js"
    },
    "./web": {
      "types": "./dist/src/web/index.d.ts",
      "default": "./dist/src/web/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc && pnpm run build:templates",
    "build:templates": "cp -r src/server/templates dist/src/server/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "keywords": [
    "chatgpt",
    "app",
    "ai",
    "skybridge"
  ],
  "author": "Frédéric Barthelet",
  "license": "ISC",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "vite": "^7.1.11",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.15.30",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/jsdom": "^21.1.6",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^25.0.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@10.17.1"
}
